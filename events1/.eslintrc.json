{
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true,
      "jest": true
    },
    "rules": {
      "no-plusplus": "off",
      "class-methods-use-this": "off",
      "quotes": ["error", "single"],
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "indent": ["error", 2]
    }
  }